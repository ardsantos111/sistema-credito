{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt"
      }
    },
    {
      "src": "db_test.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/test-db",
      "dest": "db_test.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}